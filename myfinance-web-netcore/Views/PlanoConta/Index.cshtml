@using myfinance_web_netcore.Domain
@{
    ViewData["Title"] = "Plano de Contas";
}

<h1> Plano de Contas</h1>
<br />
<button type=" button" class=" btn btn-primary"> Criar item de Plano de Conta</button>
<br /> <br />
<table class="table table-bordered">
    <tr>
        <th> Id </th>
        <th> Descrição </th>
        <th> Tipo </th>
        <th> - </th>
        <th> - </th>
    </tr>

    @{
        foreach (var item in (List<PlanoConta>)ViewBag.Lista)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nome</td>
                <td>@item.Tipo</td>
                <td>-</td>
                <td>-</td>
            </tr>

        }
    }
</table>